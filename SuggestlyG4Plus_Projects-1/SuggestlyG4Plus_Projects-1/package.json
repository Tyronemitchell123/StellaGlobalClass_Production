{
  "name": "stella-global-class-project",
  "version": "1.0.0",
  "description": "Stella Global Class Production Server with Premium Features",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "build": "next build ./src",
    "start": "set NODE_ENV=production&& node src/server.js",
    "test": "echo \"Use test:e2e to run end-to-end tests\" && exit 1",
    "test:e2e": "playwright test",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write ."
  },
  "keywords": [
    "production",
    "server",
    "premium",
    "stella",
    "global",
    "class",
    "enterprise"
  ],
  "author": "Stella Global Class Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "@typescript-eslint/parser": "^7.1.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "eslint-plugin-react": "^7.34.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}